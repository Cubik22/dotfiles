#!/bin/sh

# script called while loggin to Github
# get username and password from bitwarden

# check if database is unlocked
rbw ls > /dev/null 2>&1
if [ $? -ne 0 ]; then
	echo "please login to rbw to use git credential helper (rbw unlock)" > /dev/stderr
    exit
fi

user="Cubik22"

pass="$(rbw get github-pat)"

echo username=$user
echo password=$pass
